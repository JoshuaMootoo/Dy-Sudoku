            <ui:Button name="btn-daily" icon-image="project://database/Assets/UI%20Toolkit/Resourses/Icons/calendar.svg?fileID=7388822144124034973&amp;guid=dc848d6e845d3e04e89a24ca25dc7bae&amp;type=3#calendar" class="button button-icon" style="margin-left: 10%; margin-top: 0%; margin-right: 10%; margin-bottom: 0%;" />
            <ui:Button name="btn-pvp" icon-image="project://database/Assets/UI%20Toolkit/Resourses/Icons/two-swords.svg?fileID=7388822144124034973&amp;guid=bcf8eae85967e934bb531dd8f90ea3dd&amp;type=3#two-swords" class="button button-icon" style="margin-left: 0%; margin-top: 0%; margin-right: 0%; margin-bottom: 0%;" />
