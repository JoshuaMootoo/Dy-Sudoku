        </ui:VisualElement>
        <ui:VisualElement name="bottom-container" style="flex-grow: 1; max-height: 10%; flex-direction: row; padding-left: 10%; padding-right: 10%; padding-top: 0%; padding-bottom: 0%; margin-right: 0; align-self: auto; align-content: center; align-items: center; flex-basis: auto;">
