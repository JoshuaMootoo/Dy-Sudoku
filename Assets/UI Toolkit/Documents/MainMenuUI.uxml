            <ui:Button text="Button" name="btn-settings" class="button-icon" />
        </ui:VisualElement>
        <ui:VisualElement name="title-container" style="flex-grow: 1; justify-content: flex-end;">
            <ui:Label text="Dy-Sudoku" name="txt-title" class="title" style="align-items: center;" />
            <ui:VisualElement name="button-container" style="flex-grow: 1; flex-direction: column; justify-content: flex-end; max-height: 50%;">
