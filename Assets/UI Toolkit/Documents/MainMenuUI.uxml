<ui:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/Styles/MainMenuStyle.uss?fileID=7433441132597879392&amp;guid=24cd1251d939a474f9da005b7e36d422&amp;type=3#MainMenuStyle" />
    <ui:VisualElement name="main-container" style="flex-grow: 1;">
        <ui:VisualElement name="top-container" style="flex-grow: 1; max-height: 10%; flex-direction: row;">
            <ui:Button name="btn-player-icon" icon-image="project://database/Assets/UI%20Toolkit/Resourses/Icons/user.svg?fileID=7388822144124034973&amp;guid=67bbadff7d03cbc4c9d31809b0fb421e&amp;type=3#user" />
            <ui:VisualElement name="player-text-container" style="flex-grow: 1;">
                <ui:Label text="Player Name" name="txt-player-name" />
